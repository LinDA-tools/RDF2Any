{
  "type": "query",
  "prefixes": {
    "dc": "http://purl.org/dc/elements/1.1/",
    "ns": "http://example.org/ns#"
  },
  "queryType": "SELECT",
  "variables": [
    "?title",
    "?price"
  ],
  "where": [
    {
      "type": "group",
      "patterns": [
        {
          "type": "bgp",
          "triples": [
            {
              "subject": "?x",
              "predicate": "http://example.org/ns#price",
              "object": "?p"
            },
            {
              "subject": "?x",
              "predicate": "http://example.org/ns#discount",
              "object": "?discount"
            }
          ]
        },
        {
          "type": "bind",
          "variable": "?price",
          "expression": {
            "type": "operation",
            "operator": "*",
            "args": [
              "?p",
              {
                "type": "operation",
                "operator": "-",
                "args": [
                  "\"1\"^^http://www.w3.org/2001/XMLSchema#integer",
                  "?discount"
                ]
              }
            ]
          }
        }
      ]
    },
    {
      "type": "bgp",
      "triples": [
        {
          "subject": "?x",
          "predicate": "http://purl.org/dc/elements/1.1/title",
          "object": "?title"
        }
      ]
    },
    {
      "type": "filter",
      "expression": {
        "type": "operation",
        "operator": "<",
        "args": [
          "?price",
          "\"20\"^^http://www.w3.org/2001/XMLSchema#integer"
        ]
      }
    }
  ]
}
